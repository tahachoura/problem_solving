Problem: 


class Solution:
    def mergeTwoLists(self, A: Optional[ListNode], B: Optional[ListNode]) -> Optional[ListNode]:
        new_head=ListNode() 
        prev=ListNode() 
        while A and B:
            if A.val<=B.val:
                prev.next=A
                A=A.next
            else:
                prev.next=B
                B=B.next
            prev=prev.next
        if A: prev.next=A
        if B: prev.next=B
        return new_head.next
